    "version": "0.2.0",
    "configurations": [
        {
            "name": "Acceptance_Test_Linux_App - Container debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/Test/Acceptance_Tests/Acceptance_Test_Linux_App/Build/Acceptance_Test_Linux_App",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
